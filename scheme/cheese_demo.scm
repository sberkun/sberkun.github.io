(define (visit lst)
    (if (null? lst) (cond) (begin
        (goto (- (quotient (car lst) 1000) 450) (- 400 (modulo (car lst) 1000)))
        (visit (cdr lst))
    ))
)

(define (draw-shape lst)
    (color (car lst))
    (visit (list (car (cdr lst))))
    (begin_fill)
    (visit (cdr (cdr lst)))
    (end_fill)
)

